{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "src",
    "jsx": "react",
    "paths": {
      "components:/*": [
        "./components/*"
      ],
      "constainers:/*": [
        "./constainers/*"
      ],
      "assets:/*": [
        "./assets/*"
      ],
      "commons:/*": [
        "./commons/*"
      ]
    }
  },
  "exclude": [
    "build",
    "node_modules"
  ]
}